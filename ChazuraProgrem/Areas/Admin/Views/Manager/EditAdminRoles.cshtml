@model Dictionary<string, string>
@{
    Layout = "_ManagerLayout";
    ViewBag.Title = "Edit Admin Roles";
}
<div class="row">
    <div class="offset-sm-2 col-sm-8">
        <div class="row">
            <div class="col-sm-5">
                <div class="form-group">
                    <select id="selectUser" asp-items="@(new SelectList(Model,"Key","Value"))" class="form-control">
                        <option value="" hidden selected>Select A User</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-5">
                <div id="formEdit"></div>
            </div>
        </div>
    </div>
</div>
<div id="searching">
    <form method="post" asp-action="SearchUsers" id="searchForm">
        <input type="text" class="form-control col-sm-4" id="searchInput" name="namePart" />
    </form>
    <div id="selectDisplay">
       
    </div>
</div>
@section Scripts{
    <script src="~/js/EditAdminRolesAjax.js"></script>
    <script src="~/js/GetUserNames_ajex.js"></script>
}
